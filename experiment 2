#include <stdio.h>
int main(){
    int x,y,sum,sub,multiply,division;
    printf("enter the value of x and y:")
    scanf("%d%d",&x,&y);
    sum=x+y;
    sub=x-y;
    multiply=x*y;
    division=x%y;
    printf("sum=%d \n,sub=%d \n,multiply=%d \n,division=%d \n",sum,sub.multiply,division)
    return 0;
}
